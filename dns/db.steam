;
; BIND
;
$TTL    604800
@       IN      SOA     localhost. root.localhost. (
                              8         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;

@       IN      NS      localhost.
        IN      A       127.0.0.4
*       IN      A       127.0.0.4
